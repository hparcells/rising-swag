{
  "name": "Rising Swag Chrome",
  "version": "1.0.0",
  "description": "Generates items for Rising Swag.",
  "manifest_version": 3,
  "author": "Hunter Parcells",
  "action":{
    "default_popup": "index.html",
    "default_title": "Open Rising Swag"
